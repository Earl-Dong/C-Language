#include<stdio.h>
int main()
{
    int i,j;
    for(i=0;i<10;i++)
    {
        for(j=0;j<10;j++)
        {
            printf("Hello World ");
        }
    }
    printf("\n");
    return 0;
}
//输出100次Hello World

int main()
{
    int i = 0;
    int j = 0;
    for(;;)
    {
        for(;;)
        {
            printf("Hello World ");
        }
    }
    printf("\n");
    return 0;
}
//死循环

int main()
{
    int i = 0;
    int j = 0;
    for(;i<10;i++)
    {
        for(;j<10;j++)
        {
            printf("Hello World ");
        }
    }
    printf("\n");
    return 0;
}
//输出10次Hello World

int main()
{
    int i = 0;
    int k = 0;
    for(i=0,k=0;k=0;i++,k++)
        k++;
    return 0;
}
//不会进入循环
